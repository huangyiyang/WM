{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", [
    "prismjs", {
      "languages": ["javascript", "css", "markup", "powerShell","json"],
      "plugins": ["line-numbers","copy-to-clipboard","jsonp-highlight","normalize-whitespace"],
      "theme": "tomorrow",
      "css": true
    }]
  ]
}
